package br.com.fiap.tds;

public class OperadoresIgualdadeRelacionais {

	public static void main(String[] args) {
		
		int idade = 10;
		boolean maioridade = idade > 18;
		System.out.println(maioridade);
		
		int idade = 10;
		boolean maioridade = idade == 18;
		System.out.println(maioridade);
		
	}

}
